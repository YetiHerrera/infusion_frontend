{
  "name": "infusion_contacts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dtos": "env-cmd -x typescript-ref '$NEXT_PUBLIC_API_BASE_URL/types/typescript?MakePropertiesOptional=True' 'shared/dtos.ts'",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write --ignore-path .gitignore",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore",
    "lint:styles": "stylelint '**/*.scss'",
    "test": "jest --watchAll",
    "test-all": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "feather-icons": "^4.28.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "moment": "^2.29.1",
    "next": "^10.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-pin-field": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test-all"
    }
  },
  "license": "UNLICENSED"
}
